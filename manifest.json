{
  "name": "jQuery Shell",
  "description": "Allows jQuery command extension in current page context.",
   "icons": {
      "128": "icon128.png",
      "48": "icon48.png",
      "32": "icon32.png",
      "16": "icon16.png"
   },
  "version": "0.4",
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "icon19.png",
    "popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*", "file:///*"],
    "js": ["jquery-1.4.min.js", "content_script.js"],
    "run_at": "document_end",
    "all_frames": false
  }],
  "permissions": ["tabs", "http://*/*", "https://*/*"]
}
